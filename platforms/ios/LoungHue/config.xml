<?xml version='1.0' encoding='utf-8'?>
<widget id="io.github.derjust.lounghue" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Insomnia">
        <param name="ios-package" value="Insomnia" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <preference name="Fullscreen" value="true" />
    <icon height="180" src="www/img/favicon/apple-icon-180x180.png" width="180" />
    <icon height="60" src="www/img/favicon/apple-icon-60x60.png" width="60" />
    <icon height="120" src="www/img/favicon/apple-icon-120x120.png" width="120" />
    <icon height="76" src="www/img/favicon/apple-icon-76x76.png" width="76" />
    <icon height="152" src="www/img/favicon/apple-icon-152x152.png" width="152" />
    <icon height="57" src="www/img/favicon/apple-icon-57x57.png" width="57" />
    <icon height="114" src="www/img/favicon/apple-icon-114x114.png" width="114" />
    <icon height="72" src="www/img/favicon/apple-icon-72x72.png" width="72" />
    <icon height="144" src="www/img/favicon/apple-icon-114x114.png" width="144" />
    <splash height="960" src="www/img/favicon/Default@2x.png" width="640" />
    <splash height="1024" src="www/img/favicon/Default-Portrait.png" width="768" />
    <splash height="2048" src="www/img/favicon/Default-Portrait@2x.png" width="1536" />
    <splash height="768" src="www/img/favicon/Default-Landscape.png" width="1024" />
    <splash height="1536" src="www/img/favicon/Default-Landscape@2x.png" width="2048" />
    <splash height="1136" src="www/img/favicon/Default-568h@2x.png" width="640" />
    <splash height="1334" src="www/img/favicon/Default-667h@2x.png" width="750" />
    <splash height="2208" src="www/img/favicon/Default-Portrait-736h@3x.png" width="1242" />
    <splash height="1242" src="www/img/favicon/Default-Landscape-736h@3x.png" width="2208" />
    <name>LoungHue</name>
    <description>
        LoungHue
    </description>
    <author email="zeeman@zeeman.de" href="http://github.com/derjust/LoungHue">
        Sebastian Just
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <icon src="www/img/favicon" />
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
</widget>
